{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "examples/python/aws-cdk/templates",
  "targets": {
    "lock": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "poetry lock --no-update",
        "cwd": "examples/python/aws-cdk"
      }
    },
    "add": {
      "executor": "@nxlv/python:add",
      "options": {}
    },
    "update": {
      "executor": "@nxlv/python:update",
      "options": {}
    },
    "remove": {
      "executor": "@nxlv/python:remove",
      "options": {}
    },
    "build": {
      "executor": "@nxlv/python:build",
      "outputs": ["examples/python/aws-cdk/dist"],
      "options": {
        "outputPath": "examples/python/aws-cdk/dist",
        "publish": false
      }
    },
    "install": {
      "executor": "@nxlv/python:install",
      "options": {
        "silent": false,
        "args": "",
        "cacheDir": ".cache/pypoetry",
        "verbose": false,
        "debug": false
      }
    },
    "lint": {
      "executor": "@nxlv/python:flake8",
      "outputs": ["reports/examples/python/aws-cdk/pylint.txt"],
      "options": {
        "outputFile": "reports/examples/python/aws-cdk/pylint.txt"
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "source .venv/bin/activate && npx cdk deploy \"{args.stacks}\" -c env={args.env}"
        ],
        "cwd": "examples/python/aws-cdk",
        "parallel": false
      }
    },
    "synth": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "source .venv/bin/activate && npx cdk synth \"{args.stacks}\" -c env={args.env}"
        ],
        "cwd": "examples/python/aws-cdk",
        "parallel": false
      }
    }
  },
  "tags": []
}
